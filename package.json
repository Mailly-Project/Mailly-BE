{
  "name": "mailly-be",
  "version": "0.0.1",
  "description": "backend code for mailly service",
  "homepage": "https://github.com/Mailly-Project/Mailly-BE#readme",
  "bugs": {
    "url": "https://github.com/Mailly-Project/Mailly-BE/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mailly-Project/Mailly-BE.git"
  },
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "test": "rimraf prisma/migrations && node bin/test",
    "------------------------BUILDS------------------------": "",
    "build": "npm run build:sdk && npm run build:main && npm run build:test && npm run build:prisma",
    "build:api": "rimraf packages/api/lib && nestia all && tsc -p packages/api/tsconfig.json && rollup -c packages/api/rollup.config.js",
    "build:main": "rimraf lib && tsc",
    "build:prisma": "prisma generate --schema=prisma/schema.prisma",
    "build:sdk": "rimraf src/api/functional && nestia sdk",
    "build:swagger": "nestia swagger",
    "build:test": "rimraf bin && tsc -p test/tsconfig.json",
    "dev": "npm run build:test -- --watch",
    "eslint": "eslint src && eslint test",
    "eslint:fix": "eslint src --fix && eslint test --fix",
    "prepare": "ts-patch install && typia patch",
    "pretter": "prettier src --write && prettier test --write",
    "------------------------DEPLOYS------------------------": "",
    "start": "node lib/executable/server",
    "start:dev": "nest start --watch",
    "start:swagger": "ts-node src/executable/swagger"
  },
  "dependencies": {
    "@nestia/core": "^4.6.2",
    "@nestia/e2e": "^0.8.2",
    "@nestia/fetcher": "^4.6.2",
    "@nestia/sdk": "^4.6.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.4.1",
    "cache-manager": "^6.4.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "tstl": "^3.0.0",
    "typia": "^7.6.4"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.3.0",
    "@types/express": "^5.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "nestia": "^6.4.0",
    "prettier": "^3.5.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma-markdown": "^1.0.9",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-transform-paths": "^3.5.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
